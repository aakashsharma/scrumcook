<% if @error.present? %>
    ajax_flash_notice("<%="#{@error}"%>");
<% else %>
    $('#panels').append('<%=escape_javascript render :partial => 'projects/stories_panel', :locals => {:stories => @search_results, :category => "search", :search_term => @search_term } %>');
    <% if @search_results.present? %>
        <% @search_results.each do |story| %>
        attachTokenInput($('#<%= story.id %>_labels'));
        <% end %>
    <% end %>
<% end %>
