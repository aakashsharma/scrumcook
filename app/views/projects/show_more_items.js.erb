<% if @stories_to_append.present? %>
<% @stories_to_append.each do |story| %>
$('#<%= "#{@category}" %>_item_list_end').before('<%=escape_javascript render :partial => 'stories/story', :locals => {:story => story} %>');
attachTokenInput($('#<%= story.id %>_labels'));
<% end %>
<% else %>
$('#<%= "#{@category}" %>_infinite-scroll').detach();
<% end %>